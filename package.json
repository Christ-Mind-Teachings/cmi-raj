{
  "name": "cmi-raj",
  "version": "0.1.0",
  "description": "Teachings of Christ Mind: The Raj Material",
  "scripts": {
    "test": "jest",
    "watch": "webpack --watch",
    "jekyll": "bundle exec jekyll s",
    "start": "npm run jekyll & npm run watch",
    "d:jekyll": "JEKYLL_ENV=development bundle exec jekyll build --config ./_config.prod.yml",
    "deploy:dev": "npm run p:webpack && npm run d:jekyll",
    "p:webpack": "webpack",
    "p:jekyll": "JEKYLL_ENV=production bundle exec jekyll build --config ./_config.prod.yml",
    "deploy:prod": "npm run p:webpack && npm run p:jekyll",
    "deploy:netlify": "bundle exec jekyll s --config ./_config.netlify.yml"
  },
  "keywords": [
    "cmi"
  ],
  "author": "Rick Mercer",
  "homepage": "https://www.christmind.info/raj",
