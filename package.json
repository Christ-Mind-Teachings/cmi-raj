    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.10",
    "eslint": "^5.12.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "style-loader": "^0.20.3",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "clipboard": "^2.0.4",
    "dom-anchor-text-quote": "^4.0.2",
